     this.form.get('imagenServidor').updateValueAndValidity();\r\n                }\r\n            });\r\n        }\r\n        _handleReaderLoaded(readerEvt) {\r\n            var binaryString = readerEvt.target.result;\r\n            this.base64textString = btoa(binaryString);\r\n            this.imagePath = \"data:image/png;base64,\" + this.base64textString;\r\n            this.mostrarImagen = true;\r\n            this.ref.detectChanges();\r\n        }\r\n        reset(element) {\r\n            this.mostrarImagen = false;\r\n            this.form.patchValue({\r\n                file: null\r\n            });\r\n            element.value = \"\";\r\n        }\r\n        crearEnlace() {\r\n            this.editarEnlace(undefined);\r\n        }\r\n        editarEnlace(id) {\r\n            const modalRef = this.modalService.open(enlace_modal_component_EnlaceModalComponent, { size: 'lg' });\r\n            if (id != undefined) {\r\n                var result2 = this.listEnlaces.find(x => x.id === id);\r\n                const enlace = {\r\n                    id: result2.id,\r\n                    nombre: result2.nombre,\r\n                    url: result2.url,\r\n                };\r\n                modalRef.componentInstance.enlace = enlace;\r\n            }\r\n            else {\r\n                const enlace = {\r\n                    id: undefined,\r\n                    nombre: '',\r\n                    url: '',\r\n                };\r\n                modalRef.componentInstance.enlace = enlace;\r\n            }\r\n            modalRef.result.then((result) => {\r\n                if (result) {\r\n                    let index = this.listEnlaces.findIndex(d => d.id === id);\r\n                    if (index > -1)\r\n                        this.listEnlaces.splice(index, 1);\r\n                    this.listEnlaces.push(result);\r\n                    this.ref.detectChanges();\r\n                }\r\n            }).catch(e => {\r\n                console.log(e);\r\n            });\r\n        }\r\n        eliminarEnlace(id) {\r\n            const modalRef = this.modalService.open(eliminar_enlace_modal_component_EliminarEnlaceModalComponent, { size: 'lg' });\r\n            modalRef.componentInstance.id = id;\r\n            modalRef.result.then((result) => {\r\n                let index = this.listEnlaces.findIndex(d => d.id === id); //find index in your array\r\n                if (index > -1)\r\n                    this.listEnlaces.splice(index, 1); //remove element from array\r\n                this.ref.detectChanges();\r\n            }).catch(e => {\r\n                console.log(e);\r\n            });\r\n        }\r\n    }\r\n    NuevoComponent.ɵfac = function NuevoComponent_Factory(t) { return new (t || NuevoComponent)(core[\"Ob\" /* ɵɵdirectiveInject */](fesm2015_forms[\"d\" /* FormBuilder */]), core[\"Ob\" /* ɵɵdirectiveInject */](router[\"a\" /* ActivatedRoute */]), core[\"Ob\" /* ɵɵdirectiveInject */](router[\"h\" /* Router */]), core[\"Ob\" /* ɵɵdirectiveInject */](upload_service[\"a\" /* UploadService */]), core[\"Ob\" /* ɵɵdirectiveInject */](core[\"h\" /* ChangeDetectorRef */]), core[\"Ob\" /* ɵɵdirectiveInject */](ngx_toastr[\"b\" /* ToastrService */]), core[\"Ob\" /* ɵɵdirectiveInject */](ng_bootstrap[\"i\" /* NgbModal */]), core[\"Ob\" /* ɵɵdirectiveInject */](convenio_service[\"a\" /* ConveniosService */])); };\r\n    NuevoComponent.ɵcmp = core[\"Ib\" /* ɵɵdefineComponent */]({ type: NuevoComponent, selectors: [[\"app-nuevo\"]], viewQuery: function NuevoComponent_Query(rf, ctx) {\r\n            if (rf & 1) {\r\n                core[\"Mc\" /* ɵɵviewQuery */](_c0, true);\r\n            }\r\n            if (rf & 2) {\r\n                let _t;\r\n                core[\"vc\" /* ɵɵqueryRefresh */](_t = core[\"fc\" /* ɵɵloadQuery */]()) && (ctx.myInputVariable = _t.first);\r\n            }\r\n        }, decls: 96, vars: 10, consts: [[\"id\", \"kt_form\", 1, \"form\", 3, \"formGroup\", \"ngSubmit\"], [1, \"text-right\"],